# Example environment variables for deploying to Render.
# Copy these keys into the Render dashboard (matching render.yaml entries).

APP_ENV=production
APP_LOG_LEVEL=INFO
TZ=America/Chicago

OPENAI_API_KEY=your-openai-key
SECRET_KEY=replace-with-random-secret
ENCRYPTION_KEY=replace-with-fernet-key

# Render provisions these via linked services in render.yaml, but keep
# placeholders documented here for reference when running outside Render.
APP_DATABASE__URL=postgresql+psycopg2://username:password@host:5432/database
REDIS_URL=redis://:password@host:6379/0

# Optional integrations
RESEND_API_KEY=
RESEND_FROM_EMAIL=
EXPO_ACCESS_TOKEN=
EXPO_PUSH_URL=https://exp.host/--/api/v2/push/send

GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=https://your-domain/oauth/google/callback

STRIPE_API_KEY=
STRIPE_PRICE_PRO=
STRIPE_WEBHOOK_SECRET=
